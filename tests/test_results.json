{
    
        "node_name": "localhost",
    "datacenter": "dc1",
    "domain": "consul",
    "consul_version": "1.0.2",
            "node_meta": {"meta_1": 1, "meta_2": 2},
    
        "performance": {"raft_multiplier": 0, "leave_drain_time": "5s", "rpc_hold_timeout": "7s"},

        "bind_addr": "1.1.1.1",
    "advertise_addr": "1.1.1.1",
    "advertise_addr_wan": "1.1.1.1",
    "client_addr": "127.0.0.1",
    "addresses": {
                "dns": "127.0.0.1",
        "http": "127.0.0.1",
        "https": "127.0.0.1"
        
    },
        "ports": {
        "dns": 8600,
        "http": 8500,
        "https": -1,
                "serf_lan": 8301,
        "serf_wan": 8302,
        "server": 8300
    },

        "raft_protocol": 3,

        
        "data_dir": "/var/consul",
    "log_level": "INFO",
    "enable_syslog": true,
    "disable_update_check": false,
    "enable_script_checks": false,

                "encrypt": "consul_raw_key",
        
        
        "retry_interval": "30s",
    "retry_max": 0,
     "retry_join":         ["127.0.0.1", "{{ consul_bind_address }}"],
    
        "server": true,

        
        
                "bootstrap": true,
        
                "retry_interval_wan": "30s",
        "retry_max_wan": 0,
        
                    
        "ui": true
}
